.g-video-wrapper{
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	background-image: url(<%= asset_path 'coding.jpg' %>);
	background-size: cover;
	background-repeat: no-repeat;
	background-position: center;
	z-index: -1;
	overflow: hidden;
}
.g-video-wrapper:after{	
	content: "";
	display: block;
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	background-color: rgba(50, 55, 53, 0.7);
}
.g-video-wrapper #video{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: auto;
}
.g-scroller{
	position: absolute;
	left: 50%;
	bottom: 5.7rem;
	width: 20px;
	height: 30px;
	box-sizing: border-box;
	border: 2px solid rgba(255, 255, 255, 0.2);
	border-radius: 8px;
	transform: translateX(-50%);
}
.g-scroller:after{
	content: "";
	left: 50%;
	top: 3px;
	width: 2px;
	height: 5px;
	background-color: rgba(255, 255, 255, 0.2);
	display: block;
	position: absolute;
	transform: translateX(-50%);
	animation: scroller 1.5s infinite ease-in-out;
}
.g-main .g-wrapper{
