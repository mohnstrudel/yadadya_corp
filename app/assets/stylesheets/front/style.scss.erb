@keyframes scroller{
    0%{
        top: 0px;
        opacity:0;
    }
    18%{
        top: 3px;
        opacity:1;
    }
    82%{
        top: 13px;
        opacity:1;
    }
    100%{
        top: 16px;
        opacity:0;
    }
}


@keyframes overflowAnimate {
  from {
    height: 0;
  }

  to {
	height: 29rem; 
  }
}

.overflowAnimate {
  animation-name: overflowAnimate;
  overflow: hidden;
}
